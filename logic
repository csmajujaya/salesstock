
$("#butUNIKEN-RB-101").click(function(){
$("#serie").slideUp();
$("#varianUNIKEN-RB-101").slideDown();
})
$("#btn-backUNIKEN-RB-101").click(function(){
$("#serie").slideDown();
$("#varianUNIKEN-RB-101").slideUp();
})
